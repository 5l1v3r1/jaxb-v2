<?xml version="1.0" encoding="UTF-8"?>
<jxb:bindings
        xmlns:xsd="http://www.w3.org/2001/XMLSchema"
        xmlns:jxb="http://java.sun.com/xml/ns/jaxb"
        version="2.0">

    <jxb:bindings schemaLocation="country.xsd" node="/xsd:schema">
        <jxb:bindings node="//xsd:element[@name='population']">
            <jxb:property name="populationCount" />
        </jxb:bindings>
    </jxb:bindings>
</jxb:bindings>
